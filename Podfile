source 'https://github.com/CocoaPods/Specs.git'
abstract_target "apple-p2p-engine" do
    # use_frameworks!

    pod 'SocketRocket', '~> 0.5'
    pod 'CocoaLumberjack'
    pod "GCDWebServer", '~> 3.5'
    pod 'CocoaAsyncSocket', '~> 7.6'
    pod 'PINCache', '~> 3.0'

  target 'iOS_demo' do
    platform :ios, '10.0'
    use_frameworks!
    pod 'SnapKit'
  end

  target 'macOS_demo' do
    platform :osx, '10.12'
    use_frameworks!
    pod 'SnapKit'
  end

  target 'tvOS_demo' do
    platform :tvos, '10.2' 
    use_frameworks!
    pod 'SnapKit'
  end
end

  


